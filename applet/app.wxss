/**app.wxss**/
@font-face {
    font-family: 'PingFang SC';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('PingFangSCRegular.ttf') format('truetype'),
        url('PingFangSCRegular.svg#PingFangSCRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}
page {  
  display: block;  
  min-height: 100%;  
  background-color:  #F5F5F5; 
  font-size: 26rpx;
  font-family: 'PingFang SC';
  border-top: 1rpx solid #F5F5F5;
}  
view{
  padding: 0rpx;
  margin: 0rpx;
}
.container {
  height: 100%;
  width:100%;
  /* display: flex; */
  /* flex-direction: column; */
   /* align-items: center; */
  /* justify-content: space-between; */
  padding: 200rpx 0;
  box-sizing: border-box; 
}  

.action {
   background:rgba(39,198,140,1);
}

/* 清除浮动 */
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

/* 文字和图片居中 */
.test-image-center{
  /* 设置flex布局 */
    display: flex;
    /* 垂直居中 */
    align-items: center;
    /* 水平居中 */
    justify-content: center;
    text-align:center;
}

 /*文字隐藏*/
.introduction{
  word-break: break-all;/*允许在单词内换行*/
  text-overflow: -o-ellipsis-lastline;/*css3中webkit内核提供的一个方法类似ellipsis*/
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;/*自适应盒子*/
  -webkit-line-clamp: 1;/*此处为1行,如果是两行就改成2*/
  -webkit-box-orient: vertical;
}

/*标题吸顶操作*/
.title-fix-news{
  position: fixed;
  top:0;
  left: 0;
  z-index: 99999;
  width:100%;
    border-top: 1rpx solid #F5F5F5;
}


/* 修改页面的 */
.top-page-title{
  color:#A1A4B9;
  height:75rpx;
  padding-left: 30rpx;
  line-height:75rpx;
  font-size: 26rpx;
}

/* 保存按钮 */
.save-bottom{
  font-size: 30rpx;
  height: 80rpx;
  background:#03ce00;
  /* box-shadow:0px 8px 16px 0px #03ce00;*/
  border-radius:40rpx;
  color: #FFFFFF;
  /* 设置flex布局 */
    display: flex;
    /* 垂直居中 */
    align-items: center;
    /* 水平居中 */
    justify-content: center;
    margin: 0 auto;
}


/* 空数据处理样式 */
.empty-image{
  margin-bottom: 91rpx;
}
.empty-image-image1{
  width: 404rpx;
  text-align: center;
  margin: 0 auto;
  margin-top: 160rpx;
}
.empty-image-image2{
  width: 404rpx;
  text-align: center;
  margin: 0 auto;
  margin-top: 90rpx;
}
.empty-image-image1 image{
  width: 100%;
}
.empty-image-image2 image{
  width: 100%;
}
.empty-image-image3 image{
  width: 100%;
}
.empty-image-text{
  font-size:26rpx;
  color:rgba(102,102,102,1);
  margin-top: 39rpx;
  text-align: center;
  margin-bottom: 60rpx;
}

/* 底部技术支持 */
.jszc{
  width:100%;
  text-align: center;
  color:rgba(173,173,173,1);  
  font-size:26rpx;
  margin-bottom: 60rpx;
}